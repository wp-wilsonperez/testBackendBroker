
<%- include ../layout/header -%>

				<table class="table table-hover">
					<thead>
						<tr>
							<th>#</th>
							<th>Usuario</th>
							<th>Password</th>
							<th>Nombre</th>
						</tr>
					</thead>
					<tbody>
					<% users.forEach(function(user) {%>
						<tr class="warning">
							<td><%= user.id %></td>
							<td><%= user.username %></td>
							<td><%= user.password %></td>
							<td><%= user.firstname %></td>
						</tr>
					<% }); %>
					</tbody>
				</table>
			
<%- include ../layout/footer -%>
